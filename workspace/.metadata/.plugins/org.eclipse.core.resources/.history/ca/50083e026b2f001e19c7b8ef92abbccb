package com.acron.intro01;

public class Review4Main {

	public static void main(String[] args) {
		// 반복문
		// for(초기값; 조건; 증감문;)
		for (int i = 1; i < 50; i++) {
			// 실행문
			System.out.print(i+"번째 : ");
			if( i % 2 == 0) {
				System.out.print("짝수 ");
			}
			else {
				System.out.print("홀수 ");
			}
				
		}

	}

}
